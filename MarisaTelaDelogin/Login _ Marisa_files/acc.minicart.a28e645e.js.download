ACC.minicart={_autoload:["bindMiniCart"],bindMiniCart:function(){$(document).on("click",".js-mini-cart-link",function(i){i.preventDefault();var i=$(this).data("miniCartUrl"),t=0!=$(this).find(".js-mini-cart-count").html()?$(this).data("miniCartName"):$(this).data("miniCartEmptyName");ACC.colorbox.open(t,{href:i,maxWidth:"100%",width:"380px",initialWidth:"380px"})}),$(document).on("click",".js-mini-cart-close-button",function(i){i.preventDefault(),ACC.colorbox.close()})},updateMiniCartDisplay:function(){var t=$(".js-mini-cart-link").data("miniCartItemsText"),i=$(".js-mini-cart-link").data("miniCartRefreshUrl");$.ajax({url:i,cache:!1,type:"GET",success:function(i){$(".js-mini-cart-link .js-mini-cart-count").html('<span class="nav-items-total">'+i.miniCartCount+'<span class="items-desktop hidden-xs hidden-sm"> '+t+"</span></span>"),$(".js-mini-cart-link .js-mini-cart-price").html(i.miniCartPrice)}})}};