const breadcrumb=new class{constructor(){$(document).on("click",e=>this.toggleBreadCrumbMobile(e)),this.encapsuleBreadCrumb()}toggleBreadCrumbMobile(e){const r=$(e.target),t=$(".js-bread-first-part");r.hasClass("js-bread-first-part")||0!==r.parents(".js-bread-first-part").length?t.addClass("active"):t.removeClass("active")}encapsuleBreadCrumb(){const e=$(".js-breadcrumb").first(),t=e.find("li");if(t.length<=2)return!1;const s=[],a=[];e.find("li").each(function(e,r){(e+1>=t.length-1?a:s).push($(r)[0].outerHTML)}),$(".js-bread-first-part ul").html(s.reverse().join().replace(",","")),e.html(a.join().replace(",",""))}};